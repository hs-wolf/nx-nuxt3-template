{
  "name": "my-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/my-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi build ./apps/my-app"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi dev ./apps/my-app"
      }
    },
    "generate-pages": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi generate ./apps/my-app"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi preview ./dist/apps/my-app"
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi prepare ./apps/my-app"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx eslint --fix ./apps/my-app"
      }
    }
  },
  "tags": []
}
